{{#comments}}
    <div class="row">
        <div class="col l12 s12 m12">
            <div class="card-panel grey lighten-2">
                <i class="material-icons right" value="deleteComment" onclick="deleteComment(commentURL+'{{id}}')">close</i>

                <h5>{{author}}  ({{dateCreated}})</h5>

                <span>{{text}}</span>
            </div>
        </div>
    </div>
{{/comments}}





