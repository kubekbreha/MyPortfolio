<div class="container">
    <div class="row">
        {{#articles}}
        <div class="col l6 s12 m12">
            <div class="card">
                <div class="card-image">
                    <a href="article.html?id={{id}}">
                        <img src="{{imageLink}}" alt="{{imageLink}}"">
                    </a>
                    <span class="card-title">{{title}}</span>
                </div>
                <div class="card-content" href="article.html?id={{id}}">
                    <p>{{author}} ({{dateCreated}})</p>
                </div>
            </div>
        </div>
        {{/articles}}
    </div>
</div>





